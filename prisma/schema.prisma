generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mats {
  id        Int    @id @default(autoincrement())
  type      String
  color     String
  cellType  String
  edgeColor String
  image     String
}
